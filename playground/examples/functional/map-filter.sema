;; Functional data processing
(define people
  [{:name "Ada" :age 36}
   {:name "Bob" :age 28}
   {:name "Cat" :age 42}
   {:name "Dan" :age 31}])

(println "Names:" (map (fn (p) (:name p)) people))

(println "Over 30:"
  (filter (fn (p) (> (:age p) 30)) people))

(println "Avg age:"
  (/ (apply + (map (fn (p) (:age p)) people))
     (length people)))
