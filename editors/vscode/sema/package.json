{
  "name": "sema",
  "displayName": "Sema",
  "description": "Language support for Sema, a Lisp dialect with first-class LLM primitives",
  "version": "0.1.0",
  "publisher": "helgesverre",
  "author": {
    "name": "Helge Sverre"
  },
  "license": "MIT",
  "icon": "icon.png",
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:sema"
  ],
  "homepage": "https://sema-lang.com",
  "bugs": {
    "url": "https://github.com/helgesverre/sema/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/helgesverre/sema"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "sema",
    "lisp",
    "llm",
    "scheme",
    "s-expression",
    "ai"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "languages": [
      {
        "id": "sema",
        "aliases": [
          "Sema",
          "sema"
        ],
        "extensions": [
          ".sema"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sema",
        "scopeName": "source.sema",
        "path": "./syntaxes/sema.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "sema.clearResults",
        "title": "Sema: Clear Inline Results"
      }
    ],
    "configuration": {
      "title": "Sema",
      "properties": {
        "sema.path": {
          "type": "string",
          "default": "sema",
          "description": "Path to the sema binary"
        }
      }
    }
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0"
  }
}
