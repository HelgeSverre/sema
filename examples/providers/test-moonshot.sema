;; Test Moonshot provider (OpenAI-compatible)
(llm/configure :moonshot {:api-key (env "MOONSHOT_API_KEY")})

(define result (llm/complete "Say 'hello from Moonshot' and nothing else." {:max-tokens 20}))
(println "Moonshot complete:" result)

(print "Moonshot stream: ")
(llm/stream "Count from 1 to 5, one number per word, nothing else." {:max-tokens 30})
(newline)

(println "Moonshot: OK")
