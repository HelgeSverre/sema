;; Test Mistral provider (OpenAI-compatible, no stream_options)
(llm/configure :mistral {:api-key (env "MISTRAL_API_KEY")})

(define result
  (llm/complete "Say 'hello from Mistral' and nothing else." {:max-tokens 20}))
(println "Mistral complete:" result)

(print "Mistral stream: ")
(llm/stream "Count from 1 to 5, one number per word, nothing else."
  {:max-tokens 30})
(newline)

(println "Mistral: OK")
