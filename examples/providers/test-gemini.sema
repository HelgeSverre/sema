;; Test Google Gemini provider (unique API)
(llm/configure :gemini {:api-key (env "GOOGLE_API_KEY")})

(define result
  (llm/complete "Say 'hello from Gemini' and nothing else." {:max-tokens 20}))
(println "Gemini complete:" result)

(print "Gemini stream: ")
(llm/stream "Count from 1 to 5, one number per word, nothing else."
  {:max-tokens 30})
(newline)

(println "Gemini: OK")
