;; tak.steel â€” TAKeuchi function benchmark
;; Equivalent to tak.sema for cross-language comparison.

(define (tak x y z)
  (if (not (< y x))
      z
      (tak (tak (- x 1) y z)
           (tak (- y 1) z x)
           (tak (- z 1) x y))))

(let loop ((n 500) (v 0))
  (if (= n 0)
      (displayln v)
      (loop (- n 1) (tak 18 12 6))))
